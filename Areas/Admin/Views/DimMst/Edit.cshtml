@model Jewelly.Models.DimMst
@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPageAdmin.cshtml";
}

<h2 class="title--admin">Edit</h2>
<h6 class="sub--title">[ @Model.DimMst_ID ]</h6>
<div class="container container--left">
    <div class="row mt-5">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <form action="/Admin/DimMst/Edit/@Model.DimMst_ID" method="post">

                <div class="form-group" hidden>
                    <label class="form-text">DimSubType ID</label>
                    <input type="text" class="form-control" readonly value="@Model.DimMst_ID" name="productid" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Crt</label>
                    <input type="text" class="form-control" value="@Model.Dim_Crt" name="Dim_Crt" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Pcs</label>
                    <input type="text" class="form-control" value="@Model.Dim_Pcs" name="Dim_Pcs" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Gm</label>
                    <input type="text" class="form-control" value="@Model.Dim_Gm" name="Dim_Gm" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Size</label>
                    <input type="text" class="form-control" value="@Model.Dim_Size" name="Dim_Size" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Rate</label>
                    <input type="text" class="form-control" value="@Model.Dim_Rate" name="Dim_Rate" />
                </div>
                <div class="form-group">
                    <label class="form-text">Dim Amt</label>
                    <input type="text" class="form-control" value="@Model.Dim_Amt" name="Dim_Amt" />
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <button type="submit" class="btn mt-4 form-control edit--admin change--admin">Save Change</button>
                    </div>
                    <div class="col-md-6">
                        <a href="@Url.Action("Dim","DimMst")" class="btn mt-4 form-control edit--admin">Back</a>
                    </div>
                </div>


            </form>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>
