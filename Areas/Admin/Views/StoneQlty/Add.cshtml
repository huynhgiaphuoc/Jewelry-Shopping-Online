
@{
    ViewBag.Title = "Add";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPageAdmin.cshtml";
}
<h2 class="title--admin">Add Stone Qlty</h2>
<div class="container container--left">
    <div class="row mt-5">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <form action="/Admin/StoneQlty/Add" method="post">
                <div class="form-group">
                    <label class="form-text">StoneQltyMst</label>
                    <input type="text" class="form-control" name="StoneQlty" placeholder="" />
                </div>
                <div class="form-group">
                    <div class="row">
                        <div class="col-md-6">
                            <input type="submit" class="btn mt-4 edit--admin change--admin" name="Create" />
                        </div>
                        <div class="col-md-6">
                            <a href="@Url.Action("Stoneqlt","StoneQlty")" class="btn mt-4 edit--admin" >Back</a>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>

