@model Jewelly.Models.CertifyMst
@{
    ViewBag.Title = "Edit";
    Layout = "~/Areas/Admin/Views/Shared/_LayoutPageAdmin.cshtml";
}

<h2 class="title--admin">Edit Certify</h2>
<h6 class="sub--title">[ @Model.Certify_Type ]</h6>
<div class="container container--left">
    <div class="row mt-5">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <form action="/Admin/CertifyAdmin/Edit/@Model.Certify_ID" method="post" enctype="multipart/form-data">
                <div class="form-group">
                    <input type="hidden" value="@Model.Certify_ID" name="Certifyid" />
                    <label class="form-text">Certify</label>
                    <input type="text" class="form-control" value="@Model.Certify_Type" name="CertifyType">
                </div>
                <div class="row">
                    <div class="col-md-6">
                        <button class="btn form-control mt-4 edit--admin change--admin" name="create">Save Change</button>
                    </div>
                    <div class="col-md-6">
                        <a href="@Url.Action("Certify","CertifyAdmin")" class="btn form-control mt-4 edit--admin">Back</a>
                    </div>
                </div>

            </form>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>

